t = int(input())
for i in range(t):
  a,b,c = map(int,input().split(' '))
  if a == 0 and b == 1 or a == 0 and c == 1 or b == 0 and a == 1 or b==0 and c == 1 or c == 0 and a == 1 or c==0 and b == 1:
    print(1)
  else:
    print(0)
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
# cook your dish here
for _ in range(int(input())):
    a,b,c=map(int,input().split())
    if a==b and b==c:
        print('0')
    else:
        print('1')
        
        
        
        
        
        
        
        
        
        
        
t = int(input())
for _ in range(t):
    a,b,c = map(int,input().split())
    if a == 0 and b == 1 or a == 0 and c == 1 or b == 0 and a == 1 or b==0 and c == 1 or c == 0 and a == 1 or c==0 and b == 1:
        print(1)
    else:
        print(0)
        
        
        
        
        
        
        
        
        
        
T = int(input())
for _ in range(T):
    A,B,C = map(int,input().split())
    if A==B==C:
        print(0)
    elif A!=B or B!=C or A!=C:
        print(1)
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
# cook your dish here
n=int(input())
for i in range(1,n+1):
    a,b,c=map(int,input().split())
    if((a+b)==1 or (b+c==1) or (c+a)==1):
        print(1)
    else:
        print(0)