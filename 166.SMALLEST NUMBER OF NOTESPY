T=int(input())
for i in range(T):
    n=int(input())
    c=0
    for x in [100,50,10,5,2,1]:
        y=n//x
        n-=y*x
        c+=y
    print(c)
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
T=int(input())
for i in range(T):
    n=int(input())
    c=0
    for x in [100,50,10,5,2,1]:
        y=n//x
        n-=y*x
        c+=y
    print(c)
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
# cook your dish here\
for i in range(int(input())):
    x=int(input())
    y=x 
    count=0 
    a=[100,50,10,5,2,1]
    for i in a:
        if y%i!=i and y!=0:
            div=int(y/i)
            count+=div
            y=y-div*i 
        else:
            count+=0 
    print(count)
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
n=int(input())
for i in range(n):
    x=int(input())
    a=x%100
    b=a%50
    c=b%10
    d=c%5
    e=d%2
    f=e%1
    print ((x//100)+(a//50)+(b//10)+(c//5)+(d//2)+(e//1)+f)